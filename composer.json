{
  "name": "experience/entitle",
  "description": "AP-style title capitalisation for Craft CMS.",
  "type": "craft-plugin",
  "version": "2.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "typography"
  ],
  "support": {
    "docs": "https://github.com/experience/entitle.craft-plugin/blob/master/README.md",
    "issues": "https://github.com/experience/entitle.craft-plugin/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Experience",
      "homepage": "https://experiencehq.net"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "experience\\entitle\\": "src/",
      "experience\\entitle\\tests\\": "tests/"
    }
  },
  "extra": {
    "changelogUrl": "https://github.com/experience/entitle.craft-plugin/blob/master/CHANGELOG.md",
    "class": "experience\\entitle\\Entitle",
    "handle": "entitle",
    "name": "Entitle",
    "sourceLanguage": "en-GB"
  },
  "require-dev": {
    "codeception/codeception": "dev-master"
  }
}
